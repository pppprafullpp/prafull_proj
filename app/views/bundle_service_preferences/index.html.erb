<table cellpadding='0' cellspacing='0' class='tabbed' height='100%' width='100%'>
	<tr>
		<td class='main' id='main' valign='top'>
			<div class='title_tools'>
				<% if current_user.is_super_admin? %>
					<%#= link_to "Create New Service Preferences", new_service_preference_path %>
				<% end %>
			</div>

			<div id="users" class="list">
				<table id="users_list" class="datatable" cellpadding='0' cellspacing='0' border='1'>
					<thead>
						<tr>
							<th><strong>ID</strong></th>
							<th><strong>Service Preference Id</strong></th>
							<th><strong>Upload Speed</strong></th>
							<th><strong>Download Speed</strong></th>
							<th><strong>Data</strong></th>
							<th><strong>Free Channels</strong></th>
							<th><strong>Premium Channels</strong></th>
							<th><strong>Call Minutes</strong></th>
							<th><strong>Text Messages</strong></th>
							<th><strong>Data Plan</strong></th>
							<th><strong>Data Speed</strong></th>
							<th><strong>Unlimited Talk</strong></th>
							<th><strong>Unlimited Text</strong></th>
							<th><strong>Action</strong></th>
						</tr>
					</thead>
					<tbody>
						<% @bundle_service_preferences.each do |bsp| %>
							<% if can? :read, bsp %>
								<tr>
									<td><%= bsp.id %></td>
									<td><%= bsp.service_preference_id %></td>
									<td><%= bsp.upload_speed %></td>
									<td><%= bsp.download_speed %></td>
									<td><%= bsp.data %></td>
									<td><%= bsp.free_channels %></td>
									<td><%= bsp.premium_channels %></td>
									<td><%= bsp.call_minutes %></td>
									<td><%= bsp.text_messages %></td>
									<td><%= bsp.data_plan %></td>
									<td><%= bsp.data_speed %></td>
									<td><%= bsp.talk_unlimited %></td>
									<td><%= bsp.text_unlimited %></td>
									<td><%= (link_to 'Edit', edit_bundle_service_preference_path(bsp)) if can? :manage, bsp %> | <%= (link_to "Delete", bsp, method: :delete, data: { confirm: "You sure?" }) if can? :manage, bsp %></td>
								</tr>
							<% end %>
						<% end %>
					</tbody>
				</table>
			</div>
		</td>
	</tr>
</table>