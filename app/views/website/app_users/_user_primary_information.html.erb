

<div class="col-md-6 padding15right">
  <label for="Name" class="control-label">Primary Id<span id="mandat">*</span></label></label>
  <%= select_tag(:primary_id,options_for_select(["Driver License" , "Passport","State ID Card", "US Military Card", "US Military Department ID Card", "US Coast Guard Merchant Mariner Card", "EAD" ],selected: @app_user.try(:primary_id)),:class => 'form-control',:id => "primary_id") %>
</div>
<div id="primary_d_license" class="col-md-6">
	<%= render :partial=>"state_list", :locals => {id: 'primary_state',p_state: primary_state, name: 'primary_state'} %>
<div  class="col-md-8">
  <label for="Name" class="control-label">Primary Id Number</label>
  <%= text_field_tag(:primary_id_number1,primary_id_number,:class => 'form-control input-md',:placeholder => 'Primary Id Number', :id=> 'primary_id_number1', :maxlength=>"20") %>
</div>
</div>

<div id="primary_id_information" class="col-md-6">
  <label for="Name" class="control-label">Primary Id Number</label>
  <%= text_field_tag(:primary_id_number1,primary_id_number,:class => 'form-control input-md',:placeholder => 'Primary Id Number', :id=> 'primary_id_number1', :maxlength=>"20") %>
</div>

<div class="col-md-6">
  <label for="Name" class="control-label">Secondary Id<span id="mandat">*</span></label></label>
  <%= select_tag(:secondary_id,options_for_select(["Major credit card" , "Driver License","Passport"," State ID Card", "US Military Card", "US Military Department ID Card", "US Coast Guard Merchant Mariner Card", "EAD", "Birth certificate" ],selected: @app_user.try(:secondary_id)),:class => 'form-control',:id => "secondary_id") %>
</div>

<div id="d_license" class="col-md-6" disable =true>
	<%= render :partial=>"state_list", :locals => {id: 'secondary_state',p_state: secondary_state, name: 'secondary_state'} %>
	<div class="col-md-8">
	  <label for="Name" class="control-label">Secondary Id Number</label>
	  <%= text_field_tag(:secondary_id_number1,secondary_id_number,:class => 'form-control input-md',:placeholder => 'Secondary Id Number', :id=> 'secondary_id_number1', :maxlength=>"20") %>
	</div>
</div>

<div id="secondary_id_information" class="col-md-6">
	<label for="Name" class="control-label">Secondary Id Number</label>
	<%= text_field_tag(:secondary_id_number1,secondary_id_number,:class => 'form-control input-md',:placeholder => 'Secondary Id Number', :id=> 'secondary_id_number1', :maxlength=>"20") %>

</div>
<%= hidden_field_tag :primary_id_number,'',{:name=> 'primary_id_number'}%>
<%= hidden_field_tag :secondary_id_number,'',{:name=> 'secondary_id_number'}%>