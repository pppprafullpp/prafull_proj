<%= form_for(@telephone_service_attribute) do |f| %>
  <%= f.label :code %>
  <%= f.text_field :code, :required=>'true' %>
  
  <%= f.label :area %>
  <%= f.text_field :area %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :country %>
  <%= f.text_field :country %>
 
<% end %>