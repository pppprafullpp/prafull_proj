<div id="user_addresses" class="order-tab tab-pane fade" role="tabpanel">
    <h4>Saved Addresses</h4>
     <%if AppUser.find(session[:user_id]).user_type == "residence"%>
        <div class="col-md-12 text-center">
         <%address=AppUser.find(session[:user_id]).app_user_addresses.last%>

         <%if address.present?%>
           <div class="address-box">
            <h2><b><%=Base64.decode64(AppUser.find(session[:user_id]).first_name) + " " + Base64.decode64(AppUser.find(session[:user_id]).last_name)%></b></h2>
            <br />
            <div class="col-md-12 text-center"><%=address.address1%></div>
            <div class="col-md-12 text-center"><%=address.address2%></div>
            <div class="col-md-12 text-center"><%=address.zip%></div>
            <div class="col-md-12 text-center"><%=address.contact_number%></div>
            <div class="col-md-12 text-center"><%=address.state%></div>
            <div class="col-md-12 text-center"><%=address.city%></div>
          <%end%>
         </div>

       </div>
    <%elsif AppUser.find(session[:user_id]).user_type == "business"%>
        <p>Business user</p>
       
     <%end%>
</div>
