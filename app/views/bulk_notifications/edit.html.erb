<table cellpadding='0' cellspacing='0' class='tabbed' height='100%' width='100%' >
  <tr> 
    <td class='main' id='main' valign='top'>

      <div class='title_tools'>
				<% if current_user.is_at_admin_level? %><%= link_to "View All Sent Notifications", push_notifications_path %><% end %>
      </div>
      <div class='title'>
        <span id='title'>UPDATE</span>
      </div>
			<div class="small-6 large-centered columns">
				<%= form_for(@push_notification) do |f| %><br/>
					<div>App User Id : <br /><%= f.text_field :app_user_id %></div>
					<div>Message : <br /><%= f.text_area :message %></div>
				  <div><%= f.submit "Update" %></div>
				<% end %>
			</div>
    </td>
	</tr>
</table>